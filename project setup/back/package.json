"babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts":{
    "start": "nodemon --exec babel-node ./src/index.js",
    "build:clean":"rimraf ./build",
    "build:build": "babel src --out-dir ./build --source-maps",
    "build": "npm run build:clean && npm run build:build",
    "production": "node ./build/index.js"
  },

